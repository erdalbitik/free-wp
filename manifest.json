{
  "manifest_version": 2,

  "name": "Access wikipedia from Turkey",
  "description": "This extension makes wikipedia url accessible from Turkey which is banned currently",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
	"default_title": "Wikipedia is free"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["contentscript.js"]
    }
  ],
  "permissions": [
    "activeTab",
    "tabs"
  ]
}